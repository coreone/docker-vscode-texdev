{
  "customizations": {
    "vscode": {
      "extensions": [
        "batisteo.vscode-django",
        "bierner.markdown-preview-github-styles",
        "charliermarsh.ruff",
        "chouzz.vscode-better-align",
        "compulim.indent4to2",
        "craigmaslowski.erb",
        "davidanson.vscode-markdownlint",
        "deerawan.vscode-whitespacer",
        "donjayamanne.githistory",
        "eamodio.gitlens",
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode",
        "gitHub.vscode-github-actions",
        "hashicorp.hcl",
        "hashicorp.terraform",
        "laurenttreguier.rpm-spec",
        "ms-azuretools.vscode-containers",
        "ms-python.black-formatter",
        "ms-python.python",
        "njpwerner.autodocstring",
        "puppet.puppet-vscode",
        "riccardonovaglia.missinglineendoffile",
        "sandcastle.whitespace",
        "shopify.ruby-lsp",
        "timonwong.shellcheck",
        "tamasfe.even-better-toml",
        "wholroyd.jinja"
      ],
      "settings": {
        "files.associations": {
          "*.eyaml": "yaml",
          "Puppetfile": "ruby"
        },
        "files.watcherExclude": {
          "**/.git/objects/**": true,
          "**/.git/subtree-cache/**": true,
          "**/node_modules/*/**": true,
          "**/.hg/store/**": true,
          "**/.terragrunt-cache/**": true,
          "**/tmp/**": true
        },
        "terminal.integrated.shell.linux": "/bin/bash",
        "terraform.languageServer.ignoreDirectoryNames": [
          ".devcontainer"
        ],
        "terraform.languageServer.indexing.ignoreDirectoryNames": [
          ".devcontainer",
          ".vscode",
          ".vscode-server-insiders"
        ]
      }
    }
  },
  "image": "vscode-texdev:latest",
  "mounts": [
    "source=${localWorkspaceFolder}/.vscode-server-insiders,target=/root/.vscode-server-insiders,type=bind,consistency=cached"
  ],
  "name": "TexDev",
  "runArgs": [
      "--rm",
      "--name",
      "vscode_texdev"
  ]
}
